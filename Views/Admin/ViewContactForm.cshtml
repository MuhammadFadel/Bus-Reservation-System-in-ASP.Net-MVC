@model OnlineBusReservationV6.Models.ContactUsForm

@{
    ViewBag.Title = "View Contact Form";
}




<div class="container">
    <br /><br />
    <h2>View Contact Form</h2>
    <div class="row">
        <div class="col-md-3">
            <h2>@Model.Name</h2>
            <h4>@Model.PhoneNumber</h4>
            <h5>@Model.EmailAddress</h5>
            <p>@Model.TimeStamp</p>
        </div>
        <div class="col-md-7 col-md-offset-1">                        
            <div class="panel panel-info">
                <div class="panel-heading">Message Content</div>
                <div class="panel-body">@Model.Message</div>
            </div>
        </div>
    </div>
    <div>
        <p>
            <br /><br /><br />
            @Html.ActionLink("Back to Contact Forms", "ContactForms", null, new { @class = "btn btn-success" })
        </p>
    </div>
</div>